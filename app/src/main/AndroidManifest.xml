<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CheirinhoDeCafé"
        tools:targetApi="31">
        <activity
            android:name=".AtualizaCafe"
            android:exported="false"
            android:label="@string/title_activity_atualiza_cafe"
            android:theme="@style/Theme.CheirinhoDeCafé" />
        <activity
            android:name=".TelaListaCafe"
            android:exported="false"
            android:label="@string/title_activity_tela_lista_cafe"
            android:theme="@style/Theme.CheirinhoDeCafé" />
        <activity
            android:name=".TelaInsercao"
            android:exported="false"
            android:label="@string/title_activity_tela_insercao"
            android:theme="@style/Theme.CheirinhoDeCafé" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.CheirinhoDeCafé">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>